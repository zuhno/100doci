.slide(oncontextmenu="return false" ondragstart="return false" onselectstart="return false")
  .slide__Content.question8#q8
    .difficulty
      span 난이도
      .difficulty__stars
        img(data-src="images/svgs/fill_star.svg", alt="fill_star")
        img(data-src="images/svgs/unfill_star.svg", alt="fill_star")
        img(data-src="images/svgs/unfill_star.svg", alt="unfill_star")
    .question8__section.q8Section.q-section
      .q8Section__number.q-number
        img(data-src="https://cuttleman.github.io/bangguseok-image-hosting/mainImages/q8_top.png", alt="q8_top")
      .q8Section__text.q-text
        span.mark #[strong 아래 단어]로 #[mark 연상되는 화가]를
        br
        span 고르시오.
      .q8Section__mainImg
        img(data-src="https://cuttleman.github.io/bangguseok-image-hosting/mainImages/q8_img.png", alt="q8_img")
    .buttonContainer
      .button(data-answer="1" ontouchstart="")
        span 1. 김환기
      .button(data-answer="2" ontouchstart="")
        span 2. 이응노
      .button(data-answer="3" ontouchstart="")
        span 3. 유영국
      .button(data-answer="4" ontouchstart="")
        span 4. 이중섭